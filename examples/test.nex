#NEXUS
begin EXABAYES; 

      initGammaWeight 3
      initGammaExpWeight 3

      INITSPRWEIGHT 0 
      initEsprMappedWeight 10

      initSingleBranchWeight 3
      initSingleBranchExpWeight 3
      initSingleBranchBiunifWeight 3 


      initModelWeight 1
      initModelBiunifWeight 1
      initModelSingleBiunifWeight 1
      initModelAllBiunifWeight  1 
      initModelPermBiunifWeight 1

      initFrequenciesWeight 1


      [//PROPOSALADD nexusConfig NOTE Do not remove/modify  this line. The script addProposal.pl needs it as an identifier.]
      numGen 500000               [ total number of generations] 
      initPenaltyFactor 0.0 
      samplingFrequency 100 	[ take a sample every n generations ]

      eSprStopProb 0.3 [ stopping probability for eSPR moves ] 

      diagFreq 1000  [ check for convergence of multiple independent runs after this many generations ]
      numIndiChains 4 [ number of independant chains/runs ]

      [ MC3 ]
      numCoupledChains 1 [ number of chains per independent run (only one is cold) ]       
end;
